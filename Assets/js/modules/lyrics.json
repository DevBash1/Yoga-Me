    "1": {
        "3": {
            "type": "text",
            "text": "I'm kate, your yoga instructor."
        },
        "2": {
            "type": "action",
            "action": "speak.loadVoice();"
        },
        "7": {
            "type": "text",
            "text": "Hello {fname},\nWelcome to your first Yoga lesson."
        },
        "10": {
            "type": "text",
            "text": "Today we are going to be trying something easy and simple."
        },
        "15": {
            "type": "text",
            "text": "This is an exercise that will help you relax your mind."
        },
        "20": {
            "type": "text",
            "text": "Take the same postion as the woman in the video above."
        },
        "23": {
            "type": "action",
            "action": "play()"
        },
        "25": {
            "type": "text",
            "text": "Put both hands together and seat."
        },
        "35": {
            "type": "action",
            "action": "goto(1)"
        },
        "40": {
            "type": "text",
            "text": "Close your eyes and concentrate on my voice."
        },
        "46": {
            "type": "action",
            "action": "goto(1)"
        },
        "47": {
            "type": "text",
            "text": "Now breath in."
        },
        "50": {
            "type": "text",
            "text": "Hold your breath."
        },
        "55": {
            "type": "text",
            "text": "Now slowly breath out."
        },
        "60": {
            "type": "text",
            "text": "Continue breathing in and out."
        },
        "65": {
            "type": "text",
            "text": "Take it slow and easy."
        },
        "70": {
            "type": "text",
            "text": "Let's play you some relaxing songs."
        },
        "75": {
            "type": "action",
            "action": "playSong(0)"
        },
        "100": {
            "type": "action",
            "action": "volume(0.3)"
        },
        "101": {
            "type": "text",
            "text": "Relax your mind."
        },
        "105": {
            "type": "text",
            "text": "Imagine being alone in a garden with a river flowing beside you."
        },
        "110": {
            "type": "text",
            "text": "Inhale the aroma of the beautiful garden and feel the gentle breeze on your body."
        },
        "120": {
            "type": "action",
            "action": "volume(1)"
        },
        "200": {
            "type": "action",
            "action": "volume(0.1);speak.loadVoice();"
        },
        "205": {
            "type": "text",
            "text": "Congratulation {fname}, you have completed today's lesson."
        },
        "215": {
            "type": "action",
            "action": "endLesson()"
        }
    },
    "2": {
        "1": {
            "type": "action",
            "action": "playSong(1);speak.loadVoice();"
